{
    "For loop": {
        "prefix": "for",
        "body": [
            "For ${1:index} = ${2:lower} To ${3:upper} Step ${4:stepsize}",
            "\t$0",
            "Next"
        ],
        "description": "For loop"
    },
    "If statement": {
        "prefix": "if",
        "body": [
            "If(${1:condition}) Then",
            "\t$0",
            "EndIf"
        ],
        "description": "If statement"
    },
    "If-Else statement": {
        "prefix": "if-else",
        "body": [
            "If(${1:condition}) Then",
            "\t$2",
            "Else",
            "\t$3",
            "EndIf"
        ],
        "description": "If-Else statement"
    },
    "Subroutine": {
        "prefix": "sub",
        "body": [
            "Sub ${1:name}",
            "\t$0",
            "End Sub"
        ],
        "description": "Subroutine"
    },
    "Function": {
        "prefix": "func",
        "body": [
            "Func ${1:name}",
            "\t$0",
            "End Func"
        ],
        "description": "Function"
    },
    "While loop": {
        "prefix": "while",
        "body": [
            "While(${1:condition})",
            "\t$0",
            "Wend"
        ],
        "description": "While loop"
    },
    "Reapeat Until": {
        "prefix": "repeat",
        "body": [
            "Repeat",
            "\t$0",
            "Until(${1:condition})"
        ],
        "description": "Repeat Until"
    },
    "Select Case": {
        "prefix": "select",
        "body": [
            "Select Case ${1:name}",
            "\tCase ${1:condition}",
            "End Select"
        ],
        "description": "Select Case"
    }
}
